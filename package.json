{
  "name": "bnp",
  "version": "1.3.0-SNAPSHOT",
  "description": "Business Network Portal",
  "scripts": {
    "precommit": "npm run lint",
    "clean": "rimraf build esdoc testdb.sqlite .nyc_output coverage",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "start": "npm run build:client ; node src/server/index.js",
    "dev": "npm run setup-consul ; nodemon --watch src/server src/server/index.js",
    "setup-consul": "curl -X PUT -d ${MYSQL_DATABASE} http://${HOST_IP}:8500/v1/kv/bnp/db-init/database ; curl -X PUT -d 'root' http://${HOST_IP}:8500/v1/kv/bnp/db-init/user ; curl -X PUT -d ${MYSQL_ROOT_PASSWORD} http://${HOST_IP}:8500/v1/kv/bnp/db-init/password ; curl -X PUT -d 'true' http://${HOST_IP}:8500/v1/kv/bnp/db-init/populate-test-data",
    "esdoc": "esdoc -c esdoc.json",
    "build:client": "rimraf build && webpack --config ./webpack.production.config.js --progress --profile --colors --bail",
    "stats": "webpack --config ./webpack.production.config.js --profile --json > stats.json",
    "nsp-check": "nsp check",
    "test": "npm run clean ; npm run setup-consul ; nyc mocha --recursive -R mocha-junit-reporter"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/bnp"
  },
  "author": {
    "name": "Andrei Mourzenkov",
    "email": "andrei.mourzenkov@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "babel-loader": "6.2.8",
    "babel-runtime": "6.18.0",
    "bluebird": "^3.5.0",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "crypto-js": "3.1.8",
    "css-loader": "0.26.1",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "express-session": "1.14.2",
    "file-loader": "0.9.0",
    "fs.extra": "1.3.2",
    "handlebars": "4.0.6",
    "helmet": "3.1.0",
    "highcharts-funnel": "^0.1.2",
    "highcharts-more": "^0.1.2",
    "jquery": "3.1.1",
    "less": "2.7.1",
    "less-loader": "2.2.3",
