{
  "name": "bnp",
  "version": "2.0.0",
  "description": "Business Network Portal",
  "scripts": {
    "precommit": "npm run lint",
    "clean": "rimraf build esdoc testdb.sqlite .nyc_output coverage",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "start": "node src/server/index.js",
    "dev": "npm run setup-consul ; ${NODE_PATH}/.bin/nodemon --watch src/server src/server/index.js",
    "dev:production": "npm run setup-consul ; npm start",
    "setup-consul": "curl -X PUT -d ${MYSQL_DATABASE} http://consul:8500/v1/kv/bnp/db-init/database ; curl -X PUT -d 'root' http://consul:8500/v1/kv/bnp/db-init/user ; curl -X PUT -d ${MYSQL_ROOT_PASSWORD} http://consul:8500/v1/kv/bnp/db-init/password ; curl -X PUT -d 'true' http://consul:8500/v1/kv/bnp/db-init/populate-test-data; curl -X PUT -d ${REDIS_AUTH} http://consul:8500/v1/kv/auth/redis/password",
    "esdoc": "esdoc -c esdoc.json",
    "build:client": "${NODE_PATH}/.bin/cross-env NODE_ENV=production ; ${NODE_PATH}/.bin/webpack -p --config ./webpack.production.config.js --profile --colors --bail",
    "stats": "webpack --config ./webpack.production.config.js --profile --json > stats.json",
    "nsp-check": "nsp check",
    "test": "npm run clean ; npm run setup-consul ; nyc mocha --recursive -R mocha-junit-reporter",
    "doc": "echo \"No documentation available.\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/bnp"
  },
  "author": {
    "name": "Andrei Mourzenkov",
    "email": "andrei.mourzenkov@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "@opuscapita/i18n": "^1.1.0",
    "@opuscapita/react-loaders": "^0.1.9",
    "@opuscapita/service-base-ui": "^1.0.22",
    "bluebird": "^3.5.1",
    "cross-env": "^4.0.0",
    "highcharts-funnel": "^0.1.2",
    "highcharts-more": "^0.1.2",
    "jquery": "^3.2.1",
    "ocbesbn-config": "^2.2.11",
    "ocbesbn-db-init": "^1.1.11",
    "ocbesbn-web-init": "^2.2.9",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.2",
    "react-highcharts": "^11.5.0",
    "react-router": "^3.2.0",
    "recharts": "^0.22.4",
    "rimraf": "^2.6.2",
    "superagent": "^3.6.3",
    "superagent-bluebird-promise": "^4.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "css-loader": "^0.28.1",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^3.17.1",
    "eslint-config-opuscapita": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.11.1",
    "json-loader": "0.5.4",
    "less-loader": "^4.0.3",
    "mocha": "^3.4.1",
    "mocha-junit-reporter": "^1.13.0",
    "nodemon": "^1.12.1",
    "nsp": "^2.6.3",
    "nyc": "^10.2.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-config": "^7.5.0"
  }
}
