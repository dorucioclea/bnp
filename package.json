{
  "name": "bnp",
  "version": "2.0.1",
  "description": "Business Network Portal",
  "scripts": {
    "clean": "npx rimraf build esdoc testdb.sqlite .nyc_output coverage",
    "start": "node src/server/index.js",
    "dev": "npm run setup-consul ; npx nodemon --watch src/server src/server/index.js",
    "dev:production": "npm run setup-consul ; npm start",
    "setup-consul": "curl -X PUT -d ${MYSQL_DATABASE} http://consul:8500/v1/kv/bnp/db-init/database ; curl -X PUT -d 'root' http://consul:8500/v1/kv/bnp/db-init/user ; curl -X PUT -d ${MYSQL_ROOT_PASSWORD} http://consul:8500/v1/kv/bnp/db-init/password ; curl -X PUT -d 'true' http://consul:8500/v1/kv/bnp/db-init/populate-test-data; curl -X PUT -d ${REDIS_AUTH} http://consul:8500/v1/kv/bnp/redis/password ; curl -X PUT -d ${RABBITMQ_USER} http://consul:8500/v1/kv/bnp/mq/user ; curl -X PUT -d ${RABBITMQ_PASS} http://consul:8500/v1/kv/bnp/mq/password",
    "esdoc": "esdoc -c esdoc.json",
    "build:client": "NODE_ENV=production npx webpack -p --config ./webpack.production.config.js --profile --colors --bail",
    "stats": "webpack --config ./webpack.production.config.js --profile --json > stats.json",
    "nsp-check": "nsp check",
    "test": "npm run clean ; npm run setup-consul ; npx nyc mocha --recursive -R mocha-junit-reporter",
    "doc": "echo \"No documentation available.\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/bnp"
  },
  "author": {
    "name": "Lekealem Asong",
    "email": "Lekealem.Asong@opuscapita.com"
  },
  "license": "opuscapita",
  "readme": "README.md",
  "dependencies": {
    "@opuscapita/db-init": "^2.0.18",
    "@opuscapita/i18n": "^1.1.0",
    "@opuscapita/service-base-ui": "^1.1.113",
    "@opuscapita/web-init": "^3.0.63",
    "bluebird": "^3.5.1",
    "cross-env": "^4.0.0",
    "jquery": "^3.2.1",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.6.2",
    "react-router": "^3.2.0",
    "react-table": "^6.8.6",
    "rimraf": "^2.6.2",
    "superagent": "^3.8.2",
    "superagent-bluebird-promise": "^4.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.8.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "css-loader": "^0.28.11",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "file-loader": "^0.11.2",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "mocha": "^3.5.3",
    "mocha-junit-reporter": "^1.17.0",
    "nodemon": "^1.18.3",
    "nsp": "^2.8.1",
    "nyc": "^10.3.2",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.5.1",
    "webpack-config": "^7.5.0",
    "webpack-md5-hash": "0.0.5"
  }
}
