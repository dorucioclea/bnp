{
  "name": "bnp",
  "version": "1.3.0-SNAPSHOT",
  "description": "Business Network Portal",
  "scripts": {
    "precommit": "npm run lint",
    "clean": "rimraf build dist esdoc lib",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "start": "per-env",
    "start:development": "npm run migratedb && babel-node src/server/index.js",
    "start:production": "npm run migratedb && node build/server",
    "debug": "babel-node src/server --debug",
    "esdoc": "esdoc -c esdoc.json",
    "migratedb": "per-env",
    "migratedb:development": "./node_modules/.bin/sequelize db:migrate --url \"${DB_DIALECT}://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}\" --migrations-path src/server/db/migrations",
    "migratedb:production": "./node_modules/.bin/sequelize db:migrate --url \"${DB_DIALECT}://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}\" --migrations-path src/server/db/migrations",
    "build": "rimraf build && ./node_modules/.bin/webpack --config ./webpack.production.config.js --progress --profile --colors --bail && babel src/server -d build/server --copy-files",
    "stats": "./node_modules/.bin/webpack --config ./webpack.production.config.js --profile --json > stats.json",
    "nsp-check": "nsp check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapitaBES/bnp"
  },
  "author": {
    "name": "Andrei Mourzenkov",
    "email": "andrei.mourzenkov@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "1.15.2",
    "canvas-gauges": "2.1.0",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "1.3.0",
    "connect-multiparty": "2.0.0",
    "consul": "^0.27.0",
    "cookie-parser": "1.4.3",
    "crypto-js": "3.1.8",
    "css-loader": "0.26.1",
    "d3": "4.4.0",
    "dateformat": "2.0.0",
    "dropzone": "4.3.0",
    "email-templates": "2.5.4",
    "epilogue": "0.7.0",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "express-session": "1.14.2",
    "express-zip": "2.0.1",
    "file-loader": "0.9.0",
    "fs": "0.0.2",
    "fs.extra": "1.3.2",
    "handlebars": "4.0.6",
    "hashcode": "1.0.3",
    "helmet": "3.1.0",
    "intel": "1.1.2",
    "opuscapita-i18n": "^1.0.7",
    "jquery": "3.1.1",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "less-middleware": "2.2.0",
    "lodash": "4.17.2",
    "logrotate-stream": "0.2.5",
    "md5": "2.2.1",
    "mime-types": "2.1.13",
    "mkdirp": "0.5.1",
    "morgan": "1.7.0",
    "mysql": "2.12.0",
    "net": "1.0.2",
    "nodemailer": "2.6.4",
    "nodemailer-smtp-transport": "2.7.2",
    "on-headers": "1.0.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "per-env": "^1.0.2",
    "prettysize": "0.0.3",
    "react": "^15.4.1",
    "react-bootstrap": "0.30.7",
    "react-cookie": "1.0.4",
    "react-d3-components": "0.6.5",
    "react-dom": "15.4.1",
    "react-dropzone-component": "1.2.1",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-spinkit": "1.1.11",
    "redux": "3.6.0",
    "request": "2.79.0",
    "sequelize": "3.27.0",
    "sequelize-cli": "2.4.0",
    "terminal-kit": "^0.25.4",
    "underscore": "1.8.3",
    "url-loader": "0.5.7",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "abao": "0.5.0",
    "assets-webpack-plugin": "3.5.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "17.0.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-jest": "17.0.2",
    "babel-register": "6.18.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "chunk-manifest-webpack-plugin": "0.1.0",
    "hooks": "0.3.2",
    "husky": "^0.11.9",
    "jscs": "3.0.7",
    "cross-env": "3.1.3",
    "expect": "1.20.2",
    "eslint": "^3.10.2",
    "eslint-config-opuscapita": "^1.0.1",
    "eslint-plugin-react": "6.7.1",
    "jest-cli": "17.0.3",
    "jest-css-modules": "1.1.0",
    "jshint": "2.9.4",
    "jshint-loader": "0.8.3",
    "mocha": "3.2.0",
    "react-addons-test-utils": "15.4.1",
    "esdoc": "0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "html-webpack-plugin": "2.24.1",
    "nsp": "2.6.2",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "raml2html": "4.0.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "sqlite3": "3.1.8",
    "webpack": "^2.1.0-beta",
    "webpack-dev-middleware": "1.8.4",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "1.0.2",
    "extract-text-webpack-plugin": "^2.0.0-beta"
  },
  "jest": {
    "preprocessorIgnorePatterns": [
      "/node_modules/underscore"
    ],
    "scriptPreprocessor": "<rootDir>/test/utils/jest-preprocessor.js",
    "moduleNameMapper": {
      "\\.css$": "CSSStub"
    },
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/underscore",
      "<rootDir>/node_modules/opuscapita-i18n"
    ]
  }
}
