{
  "filePath": "./logs/sim.log",
  "formatters": {
    "simple": {
      "format": "%(date)s (%(levelname)s) [%(name)s] %(message)s",
      "colorize": true
    }
  },
  "handlers": {
    "terminal": {
      "class": "intel/handlers/console",
      "formatter": "simple"
    }
  },
  "loggers": {
    "": {
      "handlers": [
        "terminal"
      ]
    }
  }
}
